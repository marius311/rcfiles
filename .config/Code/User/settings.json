{
    "window.zoomLevel": 1,
    "editor.fontFamily": "'Iosevka Extended', 'Fira Code', 'DejaVu Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "terminal.integrated.shell.linux": "/bin/bash",
    "editor.occurrencesHighlight": false,
    "editor.renderLineHighlight": "none",
    "editor.lightbulb.enabled": false,
    "workbench.statusBar.visible": true,
    "editor.hover.enabled": false,
    "diffEditor.renderSideBySide": true,
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "python.formatting.provider": "yapf",
    "workbench.colorTheme": "Horizon",
    "editor.tokenColorCustomizations": {
        "[Horizon]": {
          "textMateRules": [
            {
              "scope": "string.docstring.julia",
              "settings": {
                "foreground": "#DEA387"
              }
            }
          ]
        }
    },
    "workbench.colorCustomizations": {
      "editor.selectionBackground": "#2E303EFF",
      "editor.selectionHighlightBackground": "#2E303EA3",
    },
    "git.confirmSync": false,
    "gitlens.views.repositories.files.layout": "tree",
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.blame.highlight.enabled": false,
    "gitlens.currentLine.scrollable": false,
    "gitlens.hovers.annotations.changes": false,
    "gitlens.hovers.annotations.details": false,
    "gitlens.hovers.annotations.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.hovers.enabled": false,
    "gitlens.views.search.enabled": false,
    "gitlens.views.lineHistory.enabled": false,
    "problems.decorations.enabled": false,
    "explorer.decorations.badges": true, 
    "explorer.decorations.colors": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "git.ignoreLegacyWarning": true,
    "gitlens.advanced.messages": {
      "suppressGitVersionWarning": true
    },
    "editor.fontLigatures": true,
    
    "julia.enableTelemetry": true,
    "julia.enableCrashReporter": true,
    "julia.executablePath": "~/bin/julia",
    "julia.lint.run": false,
}